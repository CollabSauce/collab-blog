{
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "no-console": 2,
    "jsx-a11y/no-onchange": 0,
    "jsx-a11y/media-has-caption": 0,
    "jsx-a11y/label-has-for": 0,
    "comma-dangle": ["error", "always-multiline"]
  },
  "globals": {
    "Raven": false
  },
  "settings": {
    "react": {
      "version": "detect",
      "flowVersion": "0.89"
    }
  }
}
